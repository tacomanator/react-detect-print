{
  "name": "react-detect-print",
  "version": "0.2.0",
  "description": "Detect printing status in a react.js application.",
  "author": "Drew Goodwin <drew@drewgoodwin.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "biome format --write",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepublishOnly": "npm run lint && npm run build",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tacomanator/react-detect-print.git"
  },
  "keywords": [
    "react",
    "print",
    "media-query",
    "hook",
    "detect-print"
  ],
  "bugs": {
    "url": "https://github.com/tacomanator/react-detect-print/issues"
  },
  "homepage": "https://github.com/tacomanator/react-detect-print#readme",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@tailwindcss/postcss": "4.1.17",
    "@types/react": "19.2.7",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  }
}
